{
  "name": "msb-inventory",
  "version": "0.2.5",
  "description": "Inventarsystem für den Makerspace Bonn",
  "author": {
    "name": "Falk Kühnel",
    "email": "falk@kuehnel.eu",
    "url": "https://kuehnel.eu"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun --hot --bun vite dev --port 3000 --host 0.0.0.0",
    "dev:bg": "pkill -f 'bun.*vite.*dev' 2>/dev/null || true; mkdir -p logs && nohup bun run dev > logs/dev.log 2>&1 & echo \"Dev server started (PID: $!) - logs at logs/dev.log\"",
    "dev:logs": "tail -f logs/dev.log",
    "build": "bun --bun vite build",
    "serve": "bun --bun vite preview",
    "test": "vitest run",
    "dev:tests": "docker compose exec app bun --watch --dots test",
    "dev:stop": "docker compose down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "e2e": "bash scripts/e2e-test.sh",
    "e2e:headed": "bash scripts/e2e-test.sh --headed",
    "e2e:debug": "bash scripts/e2e-test.sh --debug",
    "e2e:ui": "bun playwright test --config=e2e/playwright.config.ts --ui",
    "e2e:report": "bun playwright show-report e2e/playwright-report",
    "e2e:seed": "bun run scripts/seed-e2e-data.ts"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/html": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.13",
    "@elysiajs/static": "^1.4.7",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-router": "^1.150.0",
    "@tanstack/react-start": "^1.150.0",
    "@types/bun": "^1.3.6",
    "@types/jimp": "^0.2.28",
    "@types/sharp": "^0.32.0",
    "ansis": "^4.2.0",
    "better-auth": "^1.4.14",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.22",
    "emblor": "^1.4.8",
    "eta": "^4.5.0",
    "jimp": "^1.6.0",
    "lucide-react": "^0.562.0",
    "mqtt": "^5.14.1",
    "next-themes": "^0.4.6",
    "pg": "^8.17.0",
    "pizzip": "^3.2.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-easy-crop": "^5.5.6",
    "react-hook-form": "^7.71.1",
    "shadcn": "^3.6.3",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "overrides": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "qs": "^6.14.1",
    "cookie": "^1.0.0",
    "hono": "^4.11.4",
    "undici": "^7.18.2",
    "diff": "^8.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@playwright/test": "^1.57.0",
    "@tanstack/devtools-vite": "^0.4.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.8",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.17",
    "bun-plugin-tailwind": "^0.1.2",
